{
  "name": "desktopclient",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "author": {
    "name": "POU",
    "url": "https://github.com/Proof-Of-Uniqueness/desktopclient"
  },
  "main": "build-electron/main.js",
  "scripts": {
    "dev": "npm-run-all --parallel dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "npm run transpile:electron && cross-env NODE_ENV=development electron . --no-sandbox",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "transpile:electron": "tsc --project src/electron/tsconfig.json && tsc --project tsconfig.preload.json",
    "dist:mac": "npm run transpile:electron && npm run build && electron-builder --mac",
    "dist:linux": "npm run transpile:electron && npm run build && electron-builder --linux --x64",
    "dist:win": "npm run transpile:electron && npm run build && electron-builder --win --x64",
    "electron:dev": "concurrently \"vite\" \"wait-on tcp:3001 && cross-env VITE_DEV_SERVER_PORT=3001 electron .\"",
    "electron:build": "npm run build && electron-builder",
    "electron:preview": "electron ."
  },
  "dependencies": {
    "@chainsafe/libp2p-noise": "^16.1.3",
    "@chainsafe/libp2p-yamux": "^7.0.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@libp2p/interface-peer-id": "^2.0.2",
    "@libp2p/mdns": "^11.0.38",
    "@libp2p/tcp": "^10.1.13",
    "@libp2p/websockets": "^9.2.13",
    "@mui/material": "^6.4.8",
    "@nodeutils/defaults-deep": "^1.1.0",
    "@tanstack/react-query": "^5.69.0",
    "@types/ws": "^8.18.1",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "crypto-js": "^4.2.0",
    "elliptic": "^6.6.1",
    "libp2p": "^2.8.8",
    "notistack": "^3.0.2",
    "os-utils": "^0.0.14",
    "process": "^0.11.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.5",
    "react-router-dom": "^7.6.0",
    "recharts": "^2.15.1",
    "sass": "^1.85.0",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "ws": "^8.18.2",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/crypto-js": "^4.2.2",
    "@types/elliptic": "^6.4.18",
    "@types/os-utils": "^0.0.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.1.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "wait-on": "^8.0.3"
  },
  "overrides": {
    "@parcel/watcher": "2.2.0"
  }
}
